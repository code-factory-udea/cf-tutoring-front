{
    "compilerOptions": {
        "jsx": "react-jsx",
        "module": "esnext",
        "moduleResolution": "node",
        "target": "esnext",
        "baseUrl": ".",
        "paths": {
            "@axios/*": ["src/axios/*"],
            "@interfaces/*": ["src/interfaces/*"],
            "@services/*": ["src/services/*"],
            "@utils/*": ["src/utils/*"],
            "@components/*": ["src/components/*"],
            "@hooks/*": ["src/hooks/*"],
            "@pages/*": ["src/pages/*"],
            "@assets/*": ["src/assets/*"],
            "@context/*": ["src/context/*"]
        }
    },
    "include": ["src"],
    "references": [
        {
            "path": "./tsconfig.node.json"
        }
    ]
}
