{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@axios/*": ["src/axios/*"],
            "@interfaces/*": ["src/interfaces/*"],
            "@services/*": ["src/services/*"],
            "@utils/*": ["src/utils/*"],
            "@components/*": ["src/components/*"],
            "@hooks/*": ["src/hooks/*"],
            "@pages/*": ["src/pages/*"],
            "@assets/*": ["src/assets/*"]
        }
    },
    "include": ["src"],
    "references": [
        {
            "path": "./tsconfig.node.json"
        }
    ]
}
